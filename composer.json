{
	"name" : "internetrix/silverstripe-irxssauth",
	"type" : "silverstripe-vendormodule",
	"description" : "Member login authentication when using an internetrix.com.au email, and the user is either an irxstaff or doesn't exist in the production website",
	"license" : "BSD-3-Clause",
	"keywords" : [
		"silverstripe",
		"authentication",
		"irxssauth"
	],
	"authors" : [{
			"name" : "Yuchen Liu",
			"email" : "yuchen.liu@internetrix.com.au"
		},{
			"name" : "Guy Watson",
			"email" : "guy.watson@internetrix.com.au"
		}
	],
	"require" : {
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/framework" : "^4",
		"silverstripe/cms" : "^4"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^3.0"
	},
	"extra": {
		"installer-name": "irxssauth",
		"expose": [
			"css",
			"javascript"
		]
	},
	"autoload": {
		"psr-4": {
			"Internetrix\\IRXSSAuth\\": "src/"
		}
	},
	"minimum-stability" : "dev"
}